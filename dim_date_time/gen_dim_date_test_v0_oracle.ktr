        <jsScript_script>
//设置中文日期

var  localeCN = java.util.Locale.SIMPLIFIED_CHINESE;

var  calendarCN = new java.util.GregorianCalendar(localeCN);

calendarCN.setTime(initial_date);

calendarCN.add(java.util.Calendar.DAY_OF_MONTH, day_sequence -1);

var  dateCN = calendarCN.getTime() ;

var  dateFormatCNShort = java.text.DateFormat.getDateInstance(java.text.DateFormat.SHORT, localeCN);

var  dateCNShortStr = dateFormatCNShort.format(dateCN);

var  dateFormatCNMedium = java.text.DateFormat.getDateInstance(java.text.DateFormat.MEDIUM, localeCN);

var  dateCNMediumStr = dateFormatCNMedium.format(dateCN);

